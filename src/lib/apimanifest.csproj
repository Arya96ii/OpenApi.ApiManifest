    <SignAssembly>True</SignAssembly>
    <Title>API Manifest</Title>
    <AssemblyName>Microsoft.OpenApi.ApiManifest</AssemblyName>
    <RootNamespace>Microsoft.OpenApi.ApiManifest</RootNamespace>
  </PropertyGroup>

</Project>
